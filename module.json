{
    "title": "Playlist Enchantment",
    "description": "Some minor tweaks to enhance the playlist experience. Drag playlists to the hotbar and enjoy some additional playlist controls. Read more on the github project page.",
    "version": "1.0.0",
    "url": "https://github.com/Plushtoast/playlistenchantment",
    "manifest": "https://raw.githubusercontent.com/Plushtoast/playlistenchantment/master/module.json",
    "download": "https://github.com/Plushtoast/playlistenchantment/archive/master.zip",
    "languages": [{
            "lang": "de",
            "name": "Deutsch",
            "path": "lang/de.json"
        },
        {
            "lang": "en",
            "name": "English",
            "path": "lang/en.json"
        }
    ],
    "esmodules": [
        "modules/init.js"
    ],
    "styles": [
        "styles/css/playlist.css"
    ],
    "id": "playlistenchantment",
    "authors": [{
        "name": "Plushtoast"
    }],
    "compatibility": {
        "minimum": "11",
        "verified": "11",
        "maximum": "11"
    },
    "name": "playlistenchantment",
    "minimumCoreVersion": "11",
    "flags": {
        "hotReload": {
            "extensions": [
                "css",
                "html",
                "hbs",
                "json"
            ],
            "paths": [
                "templates",
                "styles/css",
                "lang"
            ]
        }
    }
}